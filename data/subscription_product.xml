<odoo>
  <data noupdate="0">
    <record id="mb_sub_tmpl_club" model="sale.subscription.template">
      <field name="name">Club Media Bodega – Mensual</field>
      <field name="recurring_rule_type">monthly</field>
      <field name="recurring_interval">1</field>
      <field name="invoice_mail_template_id" eval="False"/>
      <field name="recurrence_id" ref="sale_subscription.monthly_recurrence"/>
    </record>

    <record id="product_subscription_club" model="product.template">
      <field name="name">Club Media Bodega – Suscripción Mensual</field>
      <field name="type">service</field>
      <field name="list_price">9.99</field>
      <field name="sale_ok" eval="True"/>
      <field name="recurring_invoice" eval="True"/>
      <field name="subscription_template_id" ref="mb_sub_tmpl_club"/>
      <field name="website_published" eval="True"/>
    </record>
  </data>
</odoo>
